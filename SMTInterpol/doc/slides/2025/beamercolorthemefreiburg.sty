\mode<presentation>

\colorlet{ALUred}{red!70!black}
\definecolor{ALUgreen}{rgb}{.1,.5,0}
\definecolor{ALUblue}{rgb}{0,.42,.714}
\colorlet{ALUgray}{white!90!ALUblue}


\setbeamercolor{structure}{fg=ALUblue}
\setbeamercolor{background}{bg=ALUgray}
\setbeamercolor{border}{fg=ALUgreen}
\setbeamercolor{arc}{fg=ALUblue}
\setbeamercolor*{palette primary}{fg=black,bg=ALUgray}
\setbeamercolor*{palette secondary}{fg=ALUblue,bg=ALUgray}
\setbeamercolor*{palette tertiary}{fg=white!50!black,bg=ALUgray}
\setbeamercolor*{palette quarternary}{fg=white!50!black,bg=ALUgray}

\setbeamercolor*{palette sidebar primary}{fg=black}
\setbeamercolor*{palette sidebar secondary}{fg=ALUgreen}
\setbeamercolor*{palette sidebar tertiary}{fg=white!50!black}
\setbeamercolor*{palette sidebar quarternary}{fg=white!50!black}

\setbeamercolor*{alerted text}{fg=ALUred}
\setbeamercolor*{example text}{fg=ALUgreen}
\setbeamercolor*{titlelike}{use=background,fg=ALUblue,bg=background.bg}
\setbeamercolor*{button}{fg=black,bg=ALUgray}

\setbeamercolor{block title}{use=background,fg=white,bg=ALUblue}
\setbeamercolor{block title alerted}{use=alerted text,fg=white,bg=alerted text.fg!75!black}
\setbeamercolor{block title example}{use=example text,fg=white,bg=example text.fg!75!black}
\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!10!bg}

\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!10!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!10!bg}

\setbeamercolor{navigation symbols}{fg=black!40!bg}
\setbeamercolor{navigation symbols dimmed}{fg=black!20!bg}

\mode
<all>
